CREATE TABLE public.spieniarka_probki_summary
(		
	Maszyna character varying,
        Data_poczatek timestamp without time zone,
        Data_koniec timestamp without time zone,
        Nr_zlecenia character varying,
        Nr_recepty double precision,
        Producent character varying,
        Typ character varying,
        Gestosc_zadana double precision,
        Gestosc_min double precision,
        Gestosc_srednia double precision,
        Gestosc_max double precision,
        Ilosc_zad_Surowca double precision,
        Ilosc_rzecz_Surowca double precision,
        Ilosc_rzecz_partii integer,
        Operator character varying,
        Komora character varying, 
        Nr_LOT  character varying,
        Material character varying,
        Czas_cyklu double precision,
        Silos_0 character varying,
        Czas_pary double precision,
        Wyl_poziomu integer,
        Bajpas double precision,
        Wartosc_nawazki double precision,
        Cisn_pary double precision,
        Predkosc_mieszadla double precision,
        Predkosc_sluzy_lopatk double precision,
        Klapa_suszarki double precision,
        Klapa_transportu double precision
)